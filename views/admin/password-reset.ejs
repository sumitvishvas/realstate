<!-- Bootstrap Core JavaScript -->
   <script src="../../js/bootstrap.min.js"></script>
   <script>
	  $(document).ready(()=>{
		
		$('input[type="submit"]').attr('disabled','disabled');

$('#confrim_password').bind('keyup blur',function(e){
	const password=$('#password').val();
    const cnf_password=$('#confrim_password').val();

if(password==cnf_password){
	$('input[type="submit"]').removeAttr('disabled');
	$(".alert-msg").html('');
}else if(cnf_password==password){
	$('input[type="submit"]').removeAttr('disabled');
$(".alert-msg").html('');

}else{
	$(".alert-msg").html('<b class="text-danger">Password not matched.</b>');
	$('input[type="submit"]').attr('disabled','disabled');
	
	

}
})

$('#password').bind('keyup blur',function(e){
	const password=$('#password').val();
    const cnf_password=$('#confrim_password').val();
	
	
	if(cnf_password !=''){		
if(password==cnf_password){
	$('input[type="submit"]').removeAttr('disabled');
	$(".alert-msg").html('');
}else if(cnf_password==password){
	$('input[type="submit"]').removeAttr('disabled');
$(".alert-msg").html('');

}else{
	$(".alert-msg").html('<b class="text-danger">Password not matched.</b>');
	$('input[type="submit"]').attr('disabled','disabled');
	
	
}
	}
})

	  }) 
   </script>

