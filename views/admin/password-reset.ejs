 <body class="sign-in-up">
    <section>
			<div id="page-wrapper" class="sign-in-wrapper">
				
				<div class="graphs">
					<div class="sign-in-form">
						<div class="sign-in-form-top">
                            <%if(msg) {%>
                                
                                <h4 class="alert alert-success"><%=msg%></h4>
                                <%}%>
							<p><span>Password Reset</span> <a href="/admin/login"></a></p>
							
						</div>
						<div class="signin">
							
							<form action="" method="POST" >
							<div class="log-input">
								<div class="log-input-left">
								   <input type="password" class="lock" id="password" name="password" placeholder="Enter Password Address"/>
								</div>
								
								<div class="clearfix"> </div>
							</div>
							<div class="log-input">
								<div class="log-input-left">
								   <input type="password"  class="lock" id="confrim_password" name="confrim_password" placeholder="Enter Confrim Password"/>
								</div>
								
								<div class="clearfix"> </div>
							</div>
							<span class="alert-msg"></span>
							<input type="submit" value="Password Reset">
						</form>	 
						</div>
						
